    "privacy-policy.html": {
      "settings": {
        "meta_descr": "Privacy Policy | The Forward Digital",
        "title": "Privacy Policy | The Forward Digital",
        "order": 1
      },
      "components": [
        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "background-color": "@bgColor",
